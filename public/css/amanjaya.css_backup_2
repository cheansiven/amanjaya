.gradient {
    background: -moz-linear-gradient(top, rgba(0, 0, 0, 0.8) 0%, rgba(0, 0, 0, 0.53) 100px, rgba(0, 0, 0, 0) 300px, rgba(0, 0, 0, 0) 99%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(0, 0, 0, 0.8)), color-stop(100px, rgba(0, 0, 0, 0.53)), color-stop(300px, rgba(0, 0, 0, 0)), color-stop(99%, rgba(0, 0, 0, 0)));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.8) 0%, rgba(0, 0, 0, 0.53) 100px, rgba(0, 0, 0, 0) 30%, rgba(0, 0, 0, 0) 99%);
    /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, rgba(0, 0, 0, 0.8) 0%, rgba(0, 0, 0, 0.53) 100px, rgba(0, 0, 0, 0) 300px, rgba(0, 0, 0, 0) 99%);
    /* Opera 11.10+ */
    background: -ms-linear-gradient(top, rgba(0, 0, 0, 0.8) 0%, rgba(0, 0, 0, 0.53) 100px, rgba(0, 0, 0, 0) 300px, rgba(0, 0, 0, 0) 99%);
    /* IE10+ */
    background: linear-gradient(to bottom, rgba(0, 0, 0, 0.8) 0%, rgba(0, 0, 0, 0.53) 100px, rgba(0, 0, 0, 0) 300px, rgba(0, 0, 0, 0) 99%);
    /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#a6000000', endColorstr='#00000000', GradientType=0); /* IE6-9 */
}

